{
    "name": "dhii/i18n-interface",
    "description": "Interfaces for internationalization.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
        "vimeo/psalm": "^3.11.7",
        "slevomat/coding-standard": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\I18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\I18n\\Test\\Func\\": "tests/functional",
            "Dhii\\I18n\\Test\\Stub\\": "tests/stub"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev",
            "dev-0.3.x": "0.3.x-dev"
        }
    }
}
